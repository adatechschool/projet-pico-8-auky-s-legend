function life()
	 for i=1,p.life do
 	camera()spr (8, (i-1)*8, 8)
end
	for i=1,p.keys do
	camera()spr(102,0)
	end
--obstacle
 if fget(mget(p.x/8, p.y/8), 2) then
 		if not p.invincible then
			 p.life-=1
		  p.invincible=true
		 end
		end
		 if p.life==0 then
 scene="gameover"
 sfx(1)
 end
  --enemie
 if fget(mget(p.x/8, p.y/8), 3) then
 	p.keys+=1
 	p.invincible=true
 end
 		 if p.keys==1 then
 scene="victoire"
 sfx(2)
 end
	 if p.invincible then
 	p.inv_timer+=1
 	--on desactive apres 90 frames
 	if p.inv_timer>90 then
 		p.inv_timer=0
 		p.invincible=false
		end
end
end


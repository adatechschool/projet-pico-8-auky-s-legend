-- camera

function camera_scrolling()
	cam.x=p.x-50
	cam.y=p.y-50
	if (cam.x<world.start) then
	 cam.x=world.start
	elseif (cam.x+128>world.ending) then
	cam.x=world.ending
	end
	if (cam.y<world.start) then
	 cam.y=world.start
	elseif (cam.y+128>world.ending) then
	cam.y=world.ending
	end
	camera(cam.y,0)
	camera(cam.x,0)
end
